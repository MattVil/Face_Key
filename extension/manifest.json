{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",
  "path": "/Library/Google/Chrome/NativeMessagingHosts/com.my_company.my_application.json1",

  "browser_action": {
    "default_icon": "calculator-128.png", 
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "nativeMessaging"
    ],
    "content_scripts": [
      {
        "matches": ["http://*/*"],
        "js": ["jquery-1.7.2.min.js"],
        "run_at": "document_start"
      }
  ],
  "web_accessible_resources": ["log.js"]

}